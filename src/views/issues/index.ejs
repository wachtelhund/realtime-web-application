<h1>Issues</h1>
<h2><%- viewData.issues[0].title %></h2>
<div id="issues-list">
  <% viewData.issues.forEach((issue) => { %>
  <div data-id="<%= issue.id %>" class="card issue-card" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-title"><%- issue.title %></h5>
      <p class="card-text"><%- issue.description %></p>
      <p class="card-text"><%- issue.state %></p>
      <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
  </div>
  <% }) %>
</div>

<template id="issue-template">
  <div class="card issue-card" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-title issue-title"></h5>
      <p class="card-text issue-description"></p>
      <p class="card-text issue-state"></p>
      <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
  </div>
</template>